<document xsi:schemaLocation="http://www.cete.com http://www.cete.com/schemas/DPLX60.xsd" version="6.0" id="Document1" author="" keywords="" title="" subject="" xmlns="http://www.cete.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"><report id="Report1"><query id="Query1"><sql dataProvider="msSql" connectionString="#Global.ConnectionStrings[&quot;RecreationOutletContext&quot;]#">SELECT p.POID, v.VendorName, pol.POLineItemID, pol.RecRPC, i.Description, i.VendorItemID, pol.QtyOrdered, pol.WholesaleCost
FROM     PURCHASE_ORDER AS p 
 INNER JOIN PO_LINEITEM AS pol ON p.POID = pol.POID
 INNER JOIN ITEM AS i ON pol.RecRPC = i.RecRPC
 INNER JOIN PRODUCT_LINE pl on i.ProductLineID = pl.ProductLineID
 INNER JOIN VENDOR v on v.VendorID = pl.VendorID
WHERE p.POID = #Parameters["POID"]#
